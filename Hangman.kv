#:kivy 1.10.0

<HangmanGame>:
    canvas.before:

        Color:
            rgb: (0, 79/255, 48/255)

        Rectangle:
            pos: root.pos
            size: root.size
    
    Label:
        id: display_word
        text: root.display_word
        font_size: 50
        color: 241/255, 179/255, 16/255, 1
        center_x: root.width - root.width/3
        top: root.height/5
        
    Button:
        id: A
        text: 'A'
        font_size: 40
        color: 197/255, 18/255, 48/255, 1
        center_y: root.top - (root.height/10) * (int((ord(self.text)-65)/13)+1)
        center_x: ((ord(self.text)-65)%13) * root.width/13 + (root.width/13 - self.width/2)
        height: root.height/10
        width: root.width/13
        on_press: root.check_in_word(self)

